{
  "name": "effectivealtruism-org-static",
  "version": "1.0.0",
  "description": "Boilerplate for a Metalsmith-powered static site generator backed by Contentful",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "init": "npm run ./tasks/init.js",
    "dotenv": "node ./tasks/tools/generate-dotenv.js",
    "tools": "node ./tasks/tools.js",
    "metalsmith": "node ./tasks/metalsmith.js",
    "scripts": "node ./tasks/scripts.js",
    "styles": "node ./tasks/styles.js",
    "build": "npm run styles && npm run scripts && npm run metalsmith",
    "development": "NODE_ENV=development npm run build",
    "staging": "NODE_ENV=staging npm run build",
    "production": "NODE_ENV=production npm run build",
    "start": "npm run styles && npm run scripts && node ./tasks/server.js"
  },
  "engines": {
    "node": "8.11.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/centre-for-effective-altruism/effectivealtruism-org-static"
  },
  "author": "Sam Deere",
  "license": "ISC",
  "dependencies": {
    "async": "^2.0.1",
    "autolinker": "^3.0.5",
    "better-console": "^1.0.1",
    "bootstrap-3-typeahead": "^4.0.1",
