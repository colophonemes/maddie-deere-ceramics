    "inquirer": "^6.3.1",
    "jquery": "^3.1.0",
    "jstransformer-pug": "^0.3.0",
    "lazysizes": "^4.1.8",
    "markdown-it": "^8.0.0",
    "markdown-it-attrs": "^2.3.3",
