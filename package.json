    "lazysizesBGSet": "./node_modules/lazysizes/plugins/respimg/ls.respimg.js",
    "lazysizesRespImg": "./node_modules/lazysizes/plugins/bgset/ls.bgset.js",
    "bootstrapCollapse": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap/collapse.js",
    "bootstrapDropdown": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap/dropdown.js",
    "bootstrapTransition": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap/transition.js",
    "bootstrapModal": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap/modal.js",
    "bootstrapTooltip": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap/tooltip.js",
    "bootstrapTypeahead": "./node_modules/bootstrap-3-typeahead/bootstrap3-typeahead.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "pugify"
    ]
  },
  "browserify-shim": {
    "jQuery": "global:jQuery",
    "jquery": "global:jQuery",
    "lazysizesRespImg": "lazysizesRespImg",
    "lazysizesBGSet": "lazysizesBGSet",
    "bootstrapCollapse": "bootstrapCollapse",
    "bootstrapDropdown": "bootstrapDropdown",
    "bootstrapTransition": "bootstrapTransition",
    "bootstrapModal": "bootstrapModal",
    "bootstrapTypeahead": "bootstrapTypeahead"
  }
}
